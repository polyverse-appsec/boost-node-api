{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Serverless Offline",
            "type": "node",
            "request": "launch",
            "args": [
                "offline",
                "--noTimeout",
            ],
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/serverless",
            "env": {
                "NODE_ENV": "development",
            },
            "console": "integratedTerminal",
            "sourceMaps": true,
            "skipFiles": [
                "node_modules/@smithy/**",
            ]
        },
        {
            "name": "Upload User Project Data",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/create_project.py",
            "cwd": "${workspaceFolder}/../summarizer",
            "args": [
                "--email",
                "aaron@polyverse.com",
                "--github_uri",
                "https://github.com/polyverse-appsec/sara"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Groom OpenAI Files",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/sara_rest_cli.py",
            "cwd": "${workspaceFolder}",
            "args": [
                "--email",
                "stephen@polyverse.com",
                "--method",
                "aifiles_groom_at",
                "--data",
                "file-uHApzzvJAowzJJPxf24FrcR1",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Count OpenAI Files",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/sara_rest_cli.py",
            "cwd": "${workspaceFolder}",
            "args": [
                "--email",
                "stephen@polyverse.com",
                "--method",
                "aifiles",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "List OpenAI Assistants",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/sara_rest_cli.py",
            "cwd": "${workspaceFolder}",
            "args": [
                "--email",
                "stephen@polyverse.com",
                "--method",
                "assistants"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Groom Project Discoveries",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/sara_rest_cli.py",
            "cwd": "${workspaceFolder}",
            "args": [
                "--email",
                "root@localhost",
                "--method",
                "timer_interval"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "List Pending Discoveries",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/sara_rest_cli.py",
            "cwd": "${workspaceFolder}",
            "args": [
                "--email",
                "root@localhost",
                "--method",
                "list_pending_discoveries"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}
